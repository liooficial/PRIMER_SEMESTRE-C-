}
public static void p31(){
int i,h;
double p;
int [] nu = new int[10];
i=0;
p=1;
for(i=0;i<=9;i++){
Console.WriteLine("numero:" + i);
nu[i]=int.Parse(Console.ReadLine());
}
Console.WriteLine("numero buscar: " );
h=int.Parse(Console.ReadLine());
for(i=0;i<=9;i++){
if(h== nu[i]){
p=i;
Console.WriteLine("procicion: " +p);

}else{
p=-1;
}
}

}
public static void p32(){
int i;
double p,t;
int [] nu = new int[10];
int [] nu2 = new int[10];
double [] mult = new double[10];
i=0;
p=0;
for(i=0;i<=9;i++){
Console.WriteLine("numero:" + i);
nu[i]=int.Parse(Console.ReadLine());
}
for(i=0;i<=9;i++){
Console.WriteLine("numero:" + i);
nu2[i]=int.Parse(Console.ReadLine());
}
t=0;
for(i=0;i<=9;i++){
p=nu2[i]*nu[i];
mult[i]=p;
t=p+t;
Console.WriteLine(i + ") " + nu[i] +"*"+ nu2[i] +"=" +mult[i] );
}
Console.WriteLine("suma total: " +t);

}
public static void p33(){
int temp;
int [] nu = new int[10];
for(int i=0;i<=9;i++){
Console.WriteLine("incertar valor de vector");
nu[i]=int.Parse(Console.ReadLine());

}

for(int i=0;i<=9;i++){
for(int j=0;j<9;j++){
if(nu[i]> nu[j]){
temp=nu[i];
nu[i]=nu[j];
nu[j]=temp;
}
}
}
Console.WriteLine("mayor a menor");
for(int i=0;i<=9;i++){
Console.WriteLine(nu[i]);
}
}
public static void p34(){
//private int[,]mat;
for(int i=0;i<=6;i++){
for(int j=0;j<=6;j++){
