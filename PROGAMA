public static void p14(){
int opc;
double r,n1,n2;
Console.WriteLine("¿que tienes del circulo?");
Console.WriteLine("1)radio ");
Console.WriteLine("2)diametro");
Console.WriteLine("3)perimetro");
opc=int.Parse(Console.ReadLine());
switch( opc){
case 1:
Console.WriteLine("radio: ");
n1=int.Parse(Console.ReadLine());
r=3.1416*(n1*n1);
Console.WriteLine("area");
Console.WriteLine(r);
break;
case 2:
Console.WriteLine("diametro ");
n1=int.Parse(Console.ReadLine());
n1=n1/2;
r=3.1416*(n1*n1);
Console.WriteLine("area");
Console.WriteLine(r);
break;
case 3:
Console.WriteLine("perimetro ");

n1=int.Parse(Console.ReadLine());
n2=(n1/3.1416)/2;
r=3.1416*(n2*n2);
Console.WriteLine("area");
Console.WriteLine(r);
break;
default:
Console.WriteLine("error");
break;
}
}
public static void p15(){
int opc;
double r,n1,n2;
Console.WriteLine("¿que triangulo tienes?");
Console.WriteLine("1)isoseles ");
Console.WriteLine("2)equilatero");
Console.WriteLine("3)escaleno");
opc=int.Parse(Console.ReadLine());
switch( opc){
case 1:
Console.WriteLine("base ");
n1=int.Parse(Console.ReadLine());
Console.WriteLine("lado ");
n2=int.Parse(Console.ReadLine());
r=n1*Math.Sqrt( ((n2*n2)-(n1*n1)/4)/2);
Console.WriteLine("area");
Console.WriteLine(r);
break;
case 2:
Console.WriteLine("base ");
n1=int.Parse(Console.ReadLine());
n2=(n1*n1)-n1;
r=n1*Math.Sqrt(n2)*n1/2;
Console.WriteLine("area");
Console.WriteLine(r);
break;
case 3:
Console.WriteLine("base ");
n1=int.Parse(Console.ReadLine());
Console.WriteLine("altura ");
n2=int.Parse(Console.ReadLine());

r=(n1*n2)/2;
Console.WriteLine("area");
Console.WriteLine(r);
break;
default:
Console.WriteLine("error");
break;
}
}
