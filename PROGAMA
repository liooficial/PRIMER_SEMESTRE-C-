public static void p210(){
string n;
int a,b,c,d,t;
a=0;
b=0;
c=0;
t=0;
n="si";
while(n=="si"){
Console.WriteLine("dame un numero");
d=int.Parse(Console.ReadLine());
if(d==0){
a=a+1;
}else{
if(d<0){
b=b+1;
}else{
c=c+1;
}

}
Console.WriteLine("quieres introducir un nuevo numero si/no");
n=Console.ReadLine();
t=a+b+c;
}
Console.WriteLine("iguales a 0:"+a);
Console.WriteLine("menores a 0:"+b);
Console.WriteLine("mayores a 0:"+c);
Console.WriteLine("mumeros totales:"+t);
}
