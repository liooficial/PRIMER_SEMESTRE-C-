}
public static void p110(){
int n1,p,n2;
//double n2;
Console.WriteLine("peso en kilogramos");
p=int.Parse(Console.ReadLine());
if(p<10){
Console.WriteLine("costo total es de 1000");
}else{
if(p>=10 && p<=20){
n1=p-10;
n2=1000+(n1*100);
Console.WriteLine("costo total es:");
Console.WriteLine(n2);
}else{
n1=p-20;
n2=1000+(10*100)+(n1*50);
Console.WriteLine("costo total es:");
Console.WriteLine(n2);
}
}
}
public static void p112(){
/*Si el año es uniformemente divisible por 4, vaya al paso 2. ...

Si el año es uniformemente divisible por 100, vaya al paso 3. ...
Si el año es uniformemente divisible por 400, vaya al paso 4. ...
El año es un año bisiesto (tiene 366 días).*/
int n1;
double r1,r2,r3;
Console.WriteLine("dame el año");
n1=int.Parse(Console.ReadLine());
r1=n1%4;
Console.WriteLine(r1);
r2=n1%100;
Console.WriteLine(r2);
r3=n1%400;
Console.WriteLine(r2);
if((r1==0) && (r2!=0) && (r3==0) ){
Console.WriteLine("el año es bisiesto");
}else{
Console.WriteLine("el año no es bisiesto");
}

}
