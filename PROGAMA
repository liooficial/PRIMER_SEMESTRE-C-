Console.WriteLine("*");
}else{
Console.WriteLine("");
}
}
Console.WriteLine("");
}
//fin
}
public static void p35(){
int[,]dato;
int[,]num;
int[,]mul;
int i,j,s;
dato=new int[4,4];
num=new int[4,4];
mul=new int[4,4];

for( i=0;i<=3;i++){
for( j=0;j<=3;j++){
Console.WriteLine("fila "+i +"columna"+j);
dato[i,j]=Convert.ToInt16(Console.ReadLine());
}
}
for( i=0;i<=3;i++){
for( j=0;j<=3;j++){
Console.WriteLine("fila "+i +"columna"+j);
num[i,j]=Convert.ToInt16(Console.ReadLine());
}
}
for( i=0;i<=3;i++){
for( j=0;j<=3;j++){
mul[i,j]=dato[i,j] * num[i,j];
Console.WriteLine(dato[i,j] +"*" +num[i,j]+ "="+ mul[i,j]);
}
}
s=0;
for( i=0;i<=3;i++){
for( j=0;j<=3;j++){
s=s+mul[i,j];
}}
Console.WriteLine("");
Console.WriteLine("suma= "+s);
}
public static void p36(){
int i,j,m,n,p,q;
int[,]dato;
dato=new int[4,4];
for( i=0;i<=3;i++){
for( j=0;j<=3;j++){
Console.WriteLine("fila "+i +"columna"+j);
dato[i,j]=Convert.ToInt16(Console.ReadLine());
}
}
m=0;
n=0;
p=0;

q=0;
for( i=0;i<=3;i++){
for( j=0;j<=3;j++){
if(dato[0,j]>m){
m=dato[i,j];
}
if(dato[1,j]>n){
n=dato[i,j];
}
if(dato[2,j]>p){
p=dato[i,j];
}
if(dato[3,j]>q){
q=dato[i,j];
}
}
}
Console.WriteLine("fila 1 "+ m);
Console.WriteLine("fila 2 " +n);
Console.WriteLine("fila 3 " +p);
Console.WriteLine("fila 4 " +q);
}
public static void p37(){
int i,j,s;
int[,]dato;
dato=new int[4,4];
for( i=0;i<=3;i++){
for( j=0;j<=3;j++){
Console.WriteLine("fila "+i +"columna"+j);
dato[i,j]=Convert.ToInt16(Console.ReadLine());
}
}
s=0;
for( i=0;i<=3;i++){
for( j=0;j<=3;j++){
if(i==j){
s=s+dato[i,j];
}
}
}
Console.WriteLine("suma diagonal "+s);

}
