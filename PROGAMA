public static void p24(){
int i,v;
double n,p,c;
v=0;
c=0;
p=0;
for( i =0; i<6; i++){
Console.WriteLine("dame un numero ya sea positivo o negativo");
n=int.Parse(Console.ReadLine());
if(n >= 0){
v=v+1;
p=n+p;
}else{
c=c+n;
}
}
p=p/v;
Console.WriteLine("promedio= " + p);
Console.WriteLine("suma=" +c);

}
