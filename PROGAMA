case 1:
p14();
break;
case 2:
rectangulo();
break;
case 3:
p15();
break;
default:
Console.WriteLine("error");
break;
}
Console.WriteLine("quieres terminar");
c=Console.ReadLine();
} while ( c != ("si") );
}
public static void p310(){
int n1,n2,r;
Console.WriteLine("dame un numero");
n1=int.Parse(Console.ReadLine());
Console.WriteLine("dame un numero");
n2=int.Parse(Console.ReadLine());
r=EsMultiplo(n1,n2);
Console.WriteLine("porque el resutado de residuo la divicion es: "+ r);
}
public static int EsMultiplo(int x,int z){
int resultado,r;

resultado=x /z;
r=x % z;
if(r==0){
Console.WriteLine("no es multipo");
}else{
Console.WriteLine("es multipo");
}
return +r;
}
public static void rectangulo(){
double area,a,b;
Console.WriteLine("base");
b=int.Parse(Console.ReadLine());
Console.WriteLine("altura");
a=int.Parse(Console.ReadLine());
area=b*a;
Console.WriteLine("area:" +area);
}
}
}
